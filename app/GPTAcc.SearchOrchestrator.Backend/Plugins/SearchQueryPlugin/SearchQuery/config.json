{
    "schema": 1,
    "type": "completion",
    "description": "Given an input from the user, generate a best one line query for the context.",
    "completion": {
        "max_tokens": 256,
        "temperature": 0.3,
        "top_p": 1,
        "presence_penalty": 0,
        "frequency_penalty": 0
    },
    "input": {
        "parameters": [
            {
                "name": "input",
                "description": "The question from the user.",
                "defaultValue": ""
            },
            {
                "name": "messages",
                "description": "The message that is used to generate the query.",
                "defaultValue": ""
            }
        ]
    },
    "output": {
        "parameters": [
            {
                "name": "output",
                "description": "The generated query.",
                "defaultValue": ""
            }
        ]
    },
    "default_backends": []
}